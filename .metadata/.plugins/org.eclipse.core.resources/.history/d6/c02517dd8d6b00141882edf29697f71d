package dbserver;

import java.rmi.Remote;
import java.rmi.RemoteException;
import java.util.Map;

public interface DBServer extends Remote {

	
	public boolean isAvaliable(short produto) throws RemoteException;
	public boolean addCarProd (short loja, short produto) throws RemoteException;
 	public Map<Short,Short[]> getCarProd(short guid) throws RemoteException;
	public Map<Short,Short[]> payCarProd(short guid) throws RemoteException;
	
}
